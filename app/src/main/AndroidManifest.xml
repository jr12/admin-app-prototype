<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.justin.adminapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainMenu"
            android:label="@string/title_activity_main_menu"
            android:parentActivityName=".LoginActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.justin.adminapp.LoginActivity" />
        </activity>
        <activity
            android:name=".Menu_Options.box1"
            android:label="@string/title_activity_box_1" >
        </activity>
        <activity
            android:name=".Menu_Options.box2"
            android:label="@string/title_activity_box_2" >
        </activity>
        <activity
            android:name=".Menu_Options.box3"
            android:label="@string/title_activity_box_3" >
        </activity>
        <activity
            android:name=".CreateNew"
            android:label="@string/title_activity_create_new" >
        </activity>
    </application>

</manifest>
